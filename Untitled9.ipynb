      "source": [
        "def generate_fibonacci(N):\n",
        "    a, b = 0, 1\n",
        "    fibonacci_sequence = []\n",
        "    for _ in range(N):\n",
        "        fibonacci_sequence.append(a)\n",
        "        a, b = b, a + b\n",
        "    print(\" \".join(map(str, fibonacci_sequence)))"
      ]
    },
